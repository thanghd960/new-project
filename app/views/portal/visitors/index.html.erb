<h1>Portal::Visitors#index</h1>
<% @visitors.each do |visitor| %>
    <p>Tên: <%= visitor.fullname %></p>
    <p>Email: <%= visitor.email %></p>
    <p>Tạo vào: <%= time_ago visitor.created_at %></p>
     <%= link_to 'Xóa', portal_visitor_path(visitor), method: :delete, data: {confirm: 'Bạn có chắc muốn xóa bài viết không?'} %>
    <hr>
<% end %>
<p><%= will_paginate @visitors %> </p>